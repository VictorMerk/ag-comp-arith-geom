---
layout: null
---
[
  {% for page in site.pages %}
    {
      "title": "{{ page.title | escape }}",
      "url": "{{ page.url | relative_url }}",
      "content": "{{ page.content | strip_html | strip_newlines | escape }}"
    }
  {% unless forloop.last %},{% endunless %}
  {% endfor %}
  ,
  {% for post in site.posts %}
    {
      "title": "{{ post.title | escape }}",
      "url": "{{ post.url | relative_url }}",
      "content": "{{ post.content | strip_html | strip_newlines | escape }}"
    }
  {% unless forloop.last %},{% endunless %}
  {% endfor %}
  ,
  {% for member in site.members %}
    {
      "title": "{{ member.name | escape }}",
      "url": "{{ member.url | relative_url }}",
      "content": "{{ member.content | strip_html | strip_newlines | escape }} {{ member.role }} {{ member.email }}"
    }
  {% unless forloop.last %},{% endunless %}
  {% endfor %}
  ,
  {% for publication in site.publications %}
    {
      "title": "{{ publication.title | escape }}",
      "url": "{{ publication.url | relative_url }}",
      "content": "{{ publication.content | strip_html | strip_newlines | escape }} {{ publication.authors }} {{ publication.journal }}"
    }
  {% unless forloop.last %},{% endunless %}
  {% endfor %}
  ,
  {% for item in site.research %}
    {
      "title": "{{ item.title | escape }}",
      "url": "{{ item.url | relative_url }}",
      "content": "{{ item.content | strip_html | strip_newlines | escape }} {{ item.description | strip_html | strip_newlines | escape }}"
    }
  {% unless forloop.last %},{% endunless %}
  {% endfor %}
] 